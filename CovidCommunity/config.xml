<?xml version='1.0' encoding='utf-8'?>
<widget id="cmu.yenlink.covidcommunity" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Covid Community</name>
    <description>
        A contact tracing application for Covid-19.
    </description>
    <author email="yenlink@andrew.cmu.edu" href="http://cordova.io">
        Yenlin Kuo
    </author>
    <icon src="www/img/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="19" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyC5bX38mIe6wFMWFLyiVZpKQpeBVx8gjWE"/>
</widget>
